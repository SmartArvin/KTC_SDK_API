<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- combo button -->
    <style name="sound_sty_combo_btn">
    	<item name="android:layout_width">wrap_content</item>
    	<item name="android:layout_height">@dimen/dimen_sound_menu_item_height</item>
    	<item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">2dp</item>
    	<item name="android:background">@drawable/selector_second_menu_item</item>
    	<item name="android:orientation">horizontal</item>
    	<item name="android:focusable">true</item>
    	<item name="android:gravity">center_vertical</item>
    </style>
    <style name="sound_sty_combo_name">
    	<item name="android:layout_width">@dimen/dimen_sound_menu_item_name</item>
    	<item name="android:layout_height">wrap_content</item>
    	<item name="android:textColor">#FFFFFF</item>
    	<item name="android:textSize">22sp</item>
    	<item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:singleLine">true</item>
    	<item name="android:gravity">center_vertical|left</item>
    </style>
    <style name="sound_sty_combo_indicator">
    	<item name="android:layout_width">@dimen/dimen_sound_menu_item_indicator</item>
    	<item name="android:layout_height">wrap_content</item>
    	<item name="android:textColor">#FFFFFF</item>
    	<item name="android:textSize">22sp</item>
    	<item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:singleLine">true</item>
    	<item name="android:gravity">center</item>
    </style>
    
    
    <!-- seekbar button -->
    <style name="sound_sty_seekbar_btn">
    	<item name="android:layout_width">wrap_content</item>
    	<item name="android:layout_height">@dimen/dimen_sound_menu_item_height</item>
    	<item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">2dp</item>
    	<item name="android:background">@drawable/selector_second_menu_item</item>
    	<item name="android:orientation">horizontal</item>
    	<item name="android:focusable">true</item>
    	<item name="android:gravity">center_vertical</item>
    </style>
    <style name="sound_sty_bar" >
    	<item name="android:layout_width">@dimen/dimen_sound_menu_bar_width</item>
        <item name="android:layout_height">@dimen/dimen_sound_menu_bar_height</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:max">100</item>
        <item name="android:progress">50</item>
        <item name="android:thumb">@null</item>
        <item name="android:thumbOffset">0dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:progressDrawable">@drawable/second_menu_seekbar_color</item>
        <item name="android:visibility">invisible</item>
    </style>
   <style name="sound_sty_seekbar_name">
   		<item name="android:layout_width">@dimen/dimen_sound_menu_item_name</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical|left</item>
   </style>
   <style name="sound_sty_seekbar_indicator">
   		<item name="android:layout_width">@dimen/dimen_sound_menu_bar_indicator</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#FFFFFF</item>
   </style>
   
   
   <!-- button style -->
   <style name="sound_sty_button_btn">
    	<item name="android:layout_width">@dimen/dimen_sound_menu_item_width</item>
    	<item name="android:layout_height">@dimen/dimen_sound_menu_item_height</item>
    	<item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">2dp</item>
    	<item name="android:background">@drawable/selector_second_menu_item</item>
    	<item name="android:orientation">horizontal</item>
    	<item name="android:focusable">true</item>
    	<item name="android:gravity">center_vertical</item>
    </style>
    <style name="sound_sty_button_name">
    	<item name="android:layout_width">wrap_content</item>
    	<item name="android:layout_height">wrap_content</item>
    	<item name="android:textColor">#FFFFFF</item>
    	<item name="android:textSize">22sp</item>
    	<item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:singleLine">true</item>
    	<item name="android:gravity">center_vertical|left</item>
    </style>
   
   
   <!-- arrows -->
    <style name="sound_sty_leftarrow">
   		<item name="android:layout_width">@dimen/dimen_sound_menu_item_arrows_width</item>
        <item name="android:layout_height">@dimen/dimen_sound_menu_item_arrows_height</item>
        <item name="android:background">@drawable/item_left_white</item>
        <item name="android:visibility">visible</item>
   </style>
   <style name="sound_sty_rightarrow">
   		<item name="android:layout_width">@dimen/dimen_sound_menu_item_arrows_width</item>
        <item name="android:layout_height">@dimen/dimen_sound_menu_item_arrows_height</item>
        <item name="android:background">@drawable/item_right_white</item>
        <item name="android:visibility">visible</item>
   </style>
   
   
   
   
    <!-- 4 dots -->
   <style name="sound_sty_dots_layout">
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_marginLeft">133dp</item>
   		<item name="android:layout_marginTop">7dp</item>
   </style>
   <style name="sound_sty_enabledot">
   		<item name="android:layout_width">15dp</item>
   		<item name="android:layout_height">15dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   		<item name="android:background">@drawable/common_img_pagepoint_enable</item>
   </style>
   <style name="sound_sty_disabledot">
   		<item name="android:layout_width">15dp</item>
   		<item name="android:layout_height">15dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   		<item name="android:background">@drawable/common_img_pagepoint_disable</item>
   </style>
   
   <!--Search Progress Bar-->
   <style name="searchprogressbar_empty">
   		<item name="android:layout_width">7dp</item>
   		<item name="android:layout_height">10dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   </style>
   <style name="searchprogressbar_solid">
   		<item name="android:layout_width">7dp</item>
   		<item name="android:layout_height">15dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   </style>
   
   <style name="sound_sty_menu">
    	<item name="android:layout_width">275dp</item>
    	<item name="android:layout_height">336dp</item>
    	<item name="android:orientation">vertical</item>
    	<item name="android:layout_gravity">center</item>
    	<item name="android:background">@drawable/picture_mode_img_bg</item>
    </style>
    
   <!-- dots for Auto Tune Option -->	
   	 <style name="autotune_sty_dots_layout">
   		<item name="android:layout_width">wrap_content</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_marginLeft">333dp</item>
   		<item name="android:layout_marginTop">7dp</item>
   </style>
   	   <style name="autotune_sty_enabledot">
   		<item name="android:layout_width">15dp</item>
   		<item name="android:layout_height">15dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   		<item name="android:background">@drawable/common_img_pagepoint_enable</item>
   </style>
   <style name="autotune_sty_disabledot">
   		<item name="android:layout_width">15dp</item>
   		<item name="android:layout_height">15dp</item>
   		<item name="android:layout_marginLeft">4dp</item>
   		<item name="android:background">@drawable/common_img_pagepoint_disable</item>
   </style>
   	
</resources>
